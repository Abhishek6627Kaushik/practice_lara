package array_logical_coding;

import java.util.Arrays;

public class ReadFirstHalfAnArray
{
	public static void main(String[] args) {
		
	
		int[] array =  new int[8];
			array[0] =10;
			array[1] =60;
			array[2] = 90;
			array[3] = 40;
			array[4] = 80;
			array[5] = 70;
			array[6] = 65;
			array[7] = 45;
			//array[8] = 37;
			
			System.out.println(Arrays.toString(array));
	}		
		
}
