package inheritance;

class C
{
	int i ;
	static int  j;
	
	C()
	{
		System.out.println("i am cinstructor");
	}
	
	static
	{
		System.out.println("i am Sib");
	}
	
	
	{
		System.out.println("i am iib");
	}
	
	
}
public class D extends A
{
	public static void main(String[] args)
	{
		D d1 = new D();
		System.out.println(d1.test1());
	}
}
