package app27ObjectAndRefranceVariables;

public class C 
{
	 static int i ;
	
	static void test1(C c1 , C c2)
	{
		this.i =100l
		System.out.println(c1 +":"+c2);

	}
	public static void main(String[] args)
	{
	
		C c1 = new C();
		c1.i= 100;
		C c2 = new C();
		c2.i=200;
		test1(c1 , c2);
		System.out.println();
	}
}
